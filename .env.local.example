# Frontend Environment Variables
# Copy this file to .env.local and update values as needed

# Default user email for login screen (optional)
# VITE_DEFAULT_USER_EMAIL=

# Default role for login screen (admin, editor, or viewer)
VITE_DEFAULT_USER_ROLE=admin

# Default Database URL (optional, for convenience during development)
# VITE_DEFAULT_IOT_DB_URL=postgresql://user:password@host:5432/database
# VITE_DEFAULT_USER_DB_URL=postgresql://settings_user:password@host:5432/database

# API Base URL
VITE_API_BASE_URL=http://localhost:3001
