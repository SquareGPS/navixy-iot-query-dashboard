# Environment variables for Docker Compose
# Copy this file to .env and fill in the values
# DO NOT commit .env to version control

# JWT Secret for authentication (REQUIRED)
# Generate a secure secret: openssl rand -hex 32
JWT_SECRET=

# Database credentials (optional - defaults shown)
POSTGRES_DB=reports_app_db
POSTGRES_USER=danilnezhdanov
POSTGRES_PASSWORD=postgres

# Report Schema URL (optional - defaults shown)
REPORT_SCHEMA_URL=https://raw.githubusercontent.com/DanilNezhdanov/navixy-dashboard-schema/refs/heads/main/navixy/examples/hello-world-dashboard.json
